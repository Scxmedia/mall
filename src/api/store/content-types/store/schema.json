{
  "kind": "collectionType",
  "collectionName": "stores",
  "info": {
    "singularName": "store",
    "pluralName": "stores",
    "displayName": "Store"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "store_id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "blocks"
    },
    "store_code": {
      "type": "string"
    },
    "offers": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "contact": {
      "type": "biginteger"
    },
    "contacts": {
      "type": "string"
    },
    "logo": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "opening_time": {
      "type": "time"
    },
    "closing_time": {
      "type": "time"
    }
  }
}
